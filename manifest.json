{
  "manifest_version": 3,
  "name": "Hackathon Project",
  "version": "1.0.1",
  "description": "Our Description will go here.",
  "permissions": ["activeTab", "webRequest", "webNavigation", "scripting"],
  "background": { "service_worker": "scripts/background.js" },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://*/*", "http://*/*"],
      "css": ["scripts/content.css"]
    }
  ],
  "host_permissions": ["https://*/*", "http://*/*"]
}
